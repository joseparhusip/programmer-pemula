{
  "version": 2,
  "builds": [
    {
      "src": "public/index.php",
      "use": "@vercel/php"
    }
  ],
  "routes": [
    {
      "src": "/build/(.*)",
      "dest": "public/build/$1"
    },
    {
      "src": "/(css|js|images|assets|fonts|favicon.ico)(.*)",
      "dest": "public/$1$2"
    },
    {
      "src": "/(.*)",
      "dest": "public/index.php"
    }
  ]
}